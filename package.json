{
  "name": "snapix",
  "version": "0.0.2",
  "description": "",
  "author": "muzyk0",
  "private": false,
  "license": "MIT",
  "scripts": {
    "dev:main": "nest start main --debug --watch",
    "dev:notifier": "nest start notifier --debug --watch",
    "dev:storage": "nest start storage --debug --watch",
    "dev:payments": "nest start payment --debug --watch",
    "eslint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "eslint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "eslint:libs:check": "eslint \"libs/**/*.ts\"",
    "eslint:main:check": "eslint \"apps/main/{src,test}/**/*.ts\"",
    "eslint:notifier:check": "eslint \"apps/notifier/**/*.ts\"",
    "format:check": "prettier --check \"{src,apps,libs,test}/**/*.ts\"",
    "format:libs:check": "prettier --check \"libs/**/*.ts\"",
    "format:main:check": "prettier --check \"apps/main/{src,test}/**/*.ts\"",
    "format:notifier:check": "prettier --check \"apps/notifier/{src,test}/**/*.ts\"",
    "format:fix": "prettier --write \"{src,apps,libs,test}/**/*.ts\"",
    "lint:check": "yarn eslint:check && yarn format:check",
    "lint:main:check": "yarn eslint:main:check && yarn format:main:check",
    "lint:libs:check": "yarn eslint:libs:check && yarn format:libs:check",
    "lint:notifier:check": "yarn eslint:notifier:check && yarn format:notifier:check",
    "tsc:libs:check": "tsc --p libs/tsconfig.libs.json --noEmit",
    "tsc:main:check": "tsc --p apps/main/tsconfig.app.json --noEmit",
    "tsc:notifier:check": "tsc --p apps/notifier/tsconfig.app.json --noEmit",
    "test:main:e2e": "jest --config apps/main/test/jest-e2e.json -i",
    "test:main:e2e:watch": "jest --config apps/main/test/jest-e2e.json --watch -i",
    "test:main": "jest --config apps/main/jest.json",
    "test:notifier": "jest --config apps/notifier/jest.json --passWithNoTests",
    "test:storage": "jest --config apps/storage/jest.json --passWithNoTests",
    "test:payments": "jest --config apps/payments/jest.json --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "main:prisma:generate": "prisma generate --schema ./apps/main/prisma/schema.prisma",
    "start:main": "node dist/apps/main/main",
    "build:main": "nest build main",
    "start:notifier": "node dist/apps/notifier/main",
    "build:notifier": "nest build notifier",
    "start:storage": "node dist/apps/storage/main",
    "build:storage": "nest build storage",
    "start:payments": "node dist/apps/payments/main",
    "build:payments": "nest build payments"
  },
  "resolutions": {
    "string-width": "4.2.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.525.0",
    "@nestjs-modules/mailer": "^1.10.3",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/cqrs": "^10.2.6",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^10.3.0",
    "@nestjs/mongoose": "^10.0.4",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/terminus": "^10.2.0",
    "@prisma/client": "^5.10.2",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.3",
    "axios": "^1.6.5",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "date-fns": "^3.2.0",
    "handlebars": "^4.7.8",
    "lodash": "^4.17.21",
    "mongoose": "^8.2.1",
    "mqtt": "^5.3.4",
    "nest-winston": "^1.9.4",
    "nestjs-i18n": "^10.4.0",
    "newrelic": "^11.10.4",
    "nodemailer": "^6.9.8",
    "passport": "^0.7.0",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@it-incubator/prettier-config": "^0.1.2",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/passport-google-oauth": "^1.0.45",
    "@types/passport-jwt": "^4.0.0",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-perfectionist": "^2.5.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.10.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/config(|/.*)$": "<rootDir>/libs/config/src/$1",
      "^@app/prisma(|/.*)$": "<rootDir>/libs/prisma/src/$1",
      "^@app/core(|/.*)$": "<rootDir>/libs/core/src/$1"
    }
  },
  "prisma": {
    "schema": "./apps/main/prisma/schema.prisma"
  }
}
